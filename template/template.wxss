.tabbar-box{
    display: flex;
    justify-content: space-around;
    align-items: center;
    position: fixed;
    bottom: 0;
    width: 750rpx;
    height: 98rpx;
    background: #ffffff;
    -webkit-box-shadow: 1rpx 0 6rpx #e1e1e1;
    box-shadow: 1rpx 0 6rpx #e1e1e1;
    font-size:20rpx;
    color:rgba(51,51,51,1);
    font-family:PingFang-SC-Medium;
}
.tabbar-box>.active{
  color:rgba(233,96,52,1);
}
.tabbar-box>view{
  flex: 1;
  text-align: center;
   display: flex;
   flex-flow: column nowrap;
}
.tabbar-box image{
  align-self: center;
  width:42rpx;
  height:42rpx;
}
.tabbar-box .issue-box{
  position: relative;
  top: -32rpx;
  font-size: 20rpx;
  text-align: center;
  
}
.issue-box .issue-img{
   width: 106rpx;
   height: 106rpx;
 }